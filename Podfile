# Podfile

source 'https://github.com/CocoaPods/Specs.git'


target 'traumbaum iOS' do
#    use_frameworks!
print("IOS")
    platform :ios, '9.0'
	pod 'AudioKit', '~> 3.3'	#currently you need xcode7 for build
    pod 'SSZipArchive'
    pod 'RegexKitLite'
end

target 'traumbaum mac' do
print("OSX")
    platform :osx
#   pod 'SSZipArchive'
    pod 'RegexKitLite'
#   pod 'AudioKit', '~> 3.3'
    pod 'RegexKitLite'
end

target 'OnTheFly Editor OSX' do
print("OTF")
    platform :osx
    pod 'SSZipArchive'
    pod 'RegexKitLite'
#   pod 'AudioKit', '~> 3.3'
end
